{
    "name": "spryker-demo/subscription-product-oms",
    "type": "library",
    "description": "Subscription product OMS module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/kernel": "^3.70.0",
        "spryker/oms": "^11.22.1",
        "spryker/product": "^6.29.1",
        "spryker/sales": "^11.34.2",
        "spryker/zed-request": "^3.18.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "config": {
        "sort-packages": true
    }
}
